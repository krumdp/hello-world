{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ETLTEST-yernenis"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"FileServerfinanceproject_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServerfinanceproject'"
		},
		"MSI_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MSI'"
		},
		"MSIData_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MSIData'"
		},
		"Oracle_azure_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Oracle_azure'"
		},
		"Sftp1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Sftp1'"
		},
		"linkedService1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedService1'"
		},
		"msi_test_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'msi_test'"
		},
		"FileServerfinanceproject_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\fileshare.msu.edu\\itservices\\departments\\infrastructure\\hal"
		},
		"FileServerfinanceproject_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "yernenis"
		},
		"Sftp1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "S:\\Projects"
		},
		"Sftp1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "yernenis"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ETLTEST-yernenis_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"FileServerfinanceproject_password": {
						"value": "[parameters('FileServerfinanceproject_password')]"
					},
					"MSI_connectionString": {
						"value": "[parameters('MSI_connectionString')]"
					},
					"MSIData_connectionString": {
						"value": "[parameters('MSIData_connectionString')]"
					},
					"linkedService1_connectionString": {
						"value": "[parameters('linkedService1_connectionString')]"
					},
					"msi_test_connectionString": {
						"value": "[parameters('msi_test_connectionString')]"
					},
					"Oracle_azure_connectionString": {
						"value": "[parameters('Oracle_azure_connectionString')]"
					},
					"Sftp1_password": {
						"value": "[parameters('Sftp1_password')]"
					},
					"FileServerfinanceproject_properties_typeProperties_host": {
						"value": "[parameters('FileServerfinanceproject_properties_typeProperties_host')]"
					},
					"FileServerfinanceproject_properties_typeProperties_userId": {
						"value": "[parameters('FileServerfinanceproject_properties_typeProperties_userId')]"
					},
					"Sftp1_properties_typeProperties_host": {
						"value": "[parameters('Sftp1_properties_typeProperties_host')]"
					},
					"Sftp1_properties_typeProperties_userName": {
						"value": "[parameters('Sftp1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}